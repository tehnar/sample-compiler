uninstall-am:
@HAVE_OCAMLFIND_TRUE@	if test "$(SITELIBDIR)" = ""; then \
@HAVE_OCAMLFIND_TRUE@	    $(OCAMLFIND) remove $(PACKAGE); \
@HAVE_OCAMLFIND_TRUE@	else \
@HAVE_OCAMLFIND_TRUE@	    rm -fR $(SITELIBDIR)/$(PACKAGE); \
@HAVE_OCAMLFIND_TRUE@	fi
@HAVE_OCAMLFIND_FALSE@	if test "$(SITELIBDIR)" = ""; then \
@HAVE_OCAMLFIND_FALSE@	    rm -fR $(OCAMLLIB)/site-lib/$(PACKAGE); \
@HAVE_OCAMLFIND_FALSE@	else \
@HAVE_OCAMLFIND_FALSE@	    rm -fR $(SITELIBDIR)/$(PACKAGE); \
@HAVE_OCAMLFIND_FALSE@	fi

-include .depend

# generic rules
###############
%.cmi: %.mli
	$(OCAMLC) -c $(BFLAGS) $(PXFLAGS) $<

# Note: cmi <- mli should go first
%.cmi: %.ml
	$(OCAMLC) -c $(BFLAGS) $(PXFLAGS) $<

%.cmo: %.ml
	$(OCAMLC) -c $(BFLAGS) $(PXFLAGS) $<

%.o: %.ml
	$(OCAMLOPT) -c $(OFLAGS) $(STATIC) $(PXFLAGS) $<

%.cmx: %.ml
	$(OCAMLOPT) -c $(OFLAGS) $(STATIC) $(PXFLAGS) $<

%.ml: %.mll
	$(OCAMLLEX) $<

%.ml: %.mly
	$(OCAMLYACC) -v $<

%.mli: %.mly
	$(OCAMLYACC) -v $<
