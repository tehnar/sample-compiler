sum1d(a) begin
    n := arrlen(a);
    s := 0;
    for i := 0, i < n, i := i + 1 do
        s := s + a[i];
    od;
    return s;
end

sum2d(a) begin
    n := arrlen(a);
    m := arrlen(a[0]);
    s := 0;
    for i := 0, i < n, i := i + 1 do
        for j := 0, j < m, j := j + 1 do
            s := s + a[i][j];
        od;
    od;
    return s;
end

write(sum2d({[1, 2], [3, 4]}) * sum1d([1, 2, 3, 4, 5, 6, 7]) * sum1d([1, 2, 3, 4]) * (sum2d({[1, 1, 1], [1, 1, 1], [1, 2, 3]}) + (sum1d([5]) * (sum2d({[1]}) * sum2d({[3, 4], [5, 6]})))))
